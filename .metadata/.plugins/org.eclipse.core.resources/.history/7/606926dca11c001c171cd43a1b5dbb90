
public class Assignment1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int k=1;
		for(int i=0;i<=4;i++) {
			
			
			for (int j=i+1;j<=i+2;j++) {
				System.out.print( k );
				k++;
			}
			System.out.println();

}
}
}







