
public class Assignment1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
//int i=1;
//int j=2;
		for(int i=1;i<=10;i=i+4) {
			
			System.out.print(i);
			for (int j=i+1;j<=3;j++) {
			System.out.print( j );
			
			}
			System.out.println();
			for(int k=5;k<=8;k++) {
				System.out.print( k );	
			}
			System.out.println();
		}
		
	}

}
