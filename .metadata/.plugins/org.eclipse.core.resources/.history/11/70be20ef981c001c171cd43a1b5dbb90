
public class Assignment1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
//int i=1;
//int j=2;
		for(int i=1;i<=10;i=i+10) {
			
			System.out.print(i);
			for (int j=i+1;j<=4;j++) {
			System.out.print( j );
			
			}
			System.out.println();
			for(int k=5;k<=7;k++) {
				System.out.print( k );	
			}
			System.out.println();
			for(int l=8;l<=9;l++) {
				System.out.print( l );
		}
			System.out.println();
			for(int n=10;n<=10;n++) {
				System.out.print( n );
	}

}
}}